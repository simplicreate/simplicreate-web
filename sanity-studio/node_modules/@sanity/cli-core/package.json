{
  "name": "@sanity/cli-core",
  "version": "0.1.0-alpha.14",
  "description": "Sanity CLI core package",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "cli",
    "tool"
  ],
  "homepage": "https://github.com/sanity-io/cli",
  "bugs": "https://github.com/sanity-io/cli/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/cli.git",
    "directory": "packages/@sanity/cli-core"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./tree": {
      "source": "./src/_exports/tree.ts",
      "default": "./dist/_exports/tree.js"
    },
    "./ux": {
      "source": "./src/_exports/ux.ts",
      "default": "./dist/_exports/ux.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4.8.0",
    "@sanity/client": "^7.14.1",
    "@sanity/types": "^5.9.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "boxen": "^8.0.1",
    "configstore": "^7.0.0",
    "debug": "^4.4.3",
    "get-tsconfig": "^4.13.6",
    "import-meta-resolve": "^4.2.0",
    "jsdom": "^27.4.0",
    "json-lexer": "^1.2.0",
    "log-symbols": "^7.0.1",
    "ora": "^9.0.0",
    "rxjs": "^7.8.2",
    "tinyglobby": "^0.2.15",
    "tsx": "^4.21.0",
    "typeid-js": "^1.2.0",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@sanity/codegen": "^5.9.3",
    "@sanity/pkg-utils": "^10.4.4",
    "@sanity/telemetry": "^0.8.1",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@types/debug": "^4.1.12",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.33",
    "eslint": "^9.39.2",
    "publint": "^0.3.17",
    "sanity": "^5.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "@repo/package.config": "0.0.1",
    "@sanity/eslint-config-cli": "0.0.0-alpha.2",
    "@repo/tsconfig": "3.70.0"
  },
  "peerDependencies": {
    "@sanity/telemetry": ">=0.8.1 <0.9.0"
  },
  "engines": {
    "node": ">=20.19.1 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ src --ignore '**/*.test.ts'",
    "build:types": "pkg-utils build --emitDeclarationOnly",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "publint": "publint",
    "test": "vitest run",
    "posttest": "pnpm run lint",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ --watch src"
  }
}