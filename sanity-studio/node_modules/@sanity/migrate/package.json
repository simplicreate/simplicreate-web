{
  "name": "@sanity/migrate",
  "version": "5.2.5",
  "description": "Tooling for running data migrations on Sanity.io projects",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "migrate",
    "migration"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/migrate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/migrate.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "default": "./dist/_exports/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/_exports/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "sanity-migrate": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json",
    "src",
    "!src/**/__tests__/**",
    "!src/**/*.test.ts",
    "!src/**/*.tar",
    "!src/**/*.tar.gz"
  ],
  "oclif": {
    "bin": "sanity",
    "commands": "./dist/commands",
    "dirname": "sanity",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.37",
    "@sanity/cli-core": "^0.1.0-alpha.12",
    "@sanity/client": "^7.15.0",
    "@sanity/mutate": "^0.16.1",
    "@sanity/types": "^5.10.0",
    "@sanity/util": "^5.10.0",
    "arrify": "^2.0.1",
    "console-table-printer": "^2.15.0",
    "debug": "^4.4.3",
    "fast-fifo": "^1.3.2",
    "get-tsconfig": "^4.13.0",
    "groq-js": "^1.27.0",
    "lodash-es": "^4.17.23",
    "p-map": "^7.0.1",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@microsoft/api-extractor": "^7.56.3",
    "@oclif/test": "^4.1.16",
    "@sanity/cli-test": "^0.0.2-alpha.11",
    "@sanity/eslint-config-cli": "^0.0.0-alpha.2",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/debug": "^4.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "chokidar": "^5.0.0",
    "eslint": "9.39.2",
    "knip": "^5.83.1",
    "oclif": "^4.22.79",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^2.5.22",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc --delete-dir-on-start src -d dist --strip-leading-paths && pnpm run build:types",
    "postbuild": "oclif manifest",
    "build:api": "api-extractor run --local --verbose",
    "build:types": "tsc --project tsconfig.build.json && pnpm run build:api",
    "clean": "rimraf dist node_modules",
    "coverage": "vitest run --coverage",
    "depcheck": "knip",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "watch": "swc src -d dist --strip-leading-paths --watch"
  }
}